{
  "expo": {
    "name": "zapp",
    "slug": "zapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-no-background.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/logo-no-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFACD"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arjun3649.zapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/logo-no-background.png",
          "dark": {
            "image": "./assets/images/logo-no-background.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d2e7e8eb-c2e4-41fc-ab7b-36fe77be5779"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d2e7e8eb-c2e4-41fc-ab7b-36fe77be5779"
    }
  }
}
